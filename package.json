{
  "name": "nft-microservice",
  "version": "0.0.0",
  "description": "The nft-service Microservice",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "tsc": "tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon -e ts, npm run dev:start",
    "build": "npm-run-all clean tsc",
    "start": "node ./dist/app/src/index.js",
    "prod:start": "npm-run-all build start",
    "watch:debug": "nodemon --inspect=0.0.0.0 -e ts,tsx --exec node -r ts-node/register ./src/index.ts",
    "watch": "nodemon -e ts,tsx --exec node -r ts-node/register ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:coverage": "nyc --reporter=html --reporter=text --all --lines 90  npm run test",
    "test": " mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "author": "Yash Sonani",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "~3.12.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server-express": "^3.6.7",
    "async": "^3.2.0",
    "axios": "^0.26.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "cowsay": "^1.5.0",
    "dotenv": "^16.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.3",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "mongoose": "^6.3.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.6",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.16",
    "@types/cors": "^2.8.10",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chai": "^4.3.4",
    "eslint": "^8.13.0",
    "fs-extra": "^10.0.1",
    "js-yaml": "^4.1.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.2.0",
    "prisma": "~3.12.0",
    "redoc-cli": "^0.13.10",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typegraphql-prisma": "^0.20.1",
    "typescript": "^4.6.3"
  }
}
